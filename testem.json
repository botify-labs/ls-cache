{
    "framework": "mocha",
    "serve_files": [ "test/browserified.js" ],
    "src_files": [
        "lib/*.js",
        "test/*tests.js"
    ],
    "launchers": {
    },
    "before_tests": "browserify test/*tests.js -o test/browserified.js",
